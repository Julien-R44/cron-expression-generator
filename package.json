{
  "name": "starter-ts",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/Julien-R44/[name]#readme",
  "bugs": {
    "url": "https://github.com/Julien-R44/[name]/issues"
  },
  "packageManager": "pnpm@6.32.9",
  "license": "MIT",
  "author": "Julien Ripouteau <julien@ripouteau.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julien-R44/[name].git"
  },
  "funding": "https://github.com/sponsors/Julien-R44",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esmo src/index.ts",
    "test": "esmo bin/test.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.14.0",
    "@japa/assert": "^1.3.4",
    "@japa/runner": "^2.0.7",
    "@japa/spec-reporter": "^1.1.12",
    "@types/node": "^17.0.25",
    "bumpp": "^7.1.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "pnpm": "^6.32.9",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.4",
    "esmo": "^0.14.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptPackage",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": [
    "build",
    "dist"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "openapi-types"
      ]
    }
  }
}
